{
    "name": "Cookie Clearer",
    "manifest_version": 3,
    "version": "1.0",
    
    "icons": {
      "16": "/img/dtu-logo.png",
      "32": "/img/dtu-logo.png",
      "48": "/img/dtu-logo.png",
      "128": "/img/dtu-logo.png"
    },
    "permissions": ["cookies","webRequest","background","tabs","webRequestBlocking", "*://*.google.com/"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["/content-script.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
  }
}